  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6",
    "@types/react": "^18.0.21",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
