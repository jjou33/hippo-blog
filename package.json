  "pnpm": {
    "allowedDeprecatedVersions": {
      "express-graphql": "0",
      "string-similarity": "1",
      "stable": "0",
      "core-js": "2",
      "async-cache": "1",
      "request": "2",
      "uuid": "3",
      "har-validator": "5",
      "babel-eslint": "10",
      "uglify-es": "3",
      "source-map-resolve": "0",
      "resolve-url": "0",
      "source-map-url": "0",
      "urix": "0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "gatsby": "4",
        "eslint": "8",
        "react": "18"
